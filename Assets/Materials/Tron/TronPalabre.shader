			float3 tanTriplanarNormal2 = mul( ase_worldToTangent, triplanar2 );
			o.Normal = tanTriplanarNormal2;
			float4 color10 = IsGammaSpace() ? float4(0.9372549,1,0.999113,1) : float4(0.8631573,1,0.9979836,1);
